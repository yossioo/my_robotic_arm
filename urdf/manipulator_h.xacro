        </inertial> 
    </link>
    
    <!-- Joint r2 -->
    <joint name="joint_r2" type="revolute">
        <parent link="prox_r"/>
        <child link="dist_r"/>
        <origin xyz="0.052 0.0000 0.00000" rpy="0 0 0"/>
        <!--         <origin xyz="0.1200 -0.0400 0.00000" rpy="${-pi*0.5} 0 0"/> -->
        <axis xyz="0 1 0"/>                                                 
        <limit velocity="6.5" effort="1000" lower="0" upper="${pi*0.65}" />
