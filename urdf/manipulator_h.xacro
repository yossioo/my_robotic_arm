    </transmission>
    
    <!-- link 5 -->
    <link name="link5">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/link_5.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/link_5.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="grey"/>
        </visual>
        
        <inertial>
            <origin xyz="0.05642 -0.00217 0.00002" rpy="0 0 0"/>
            <!--<origin xyz="-0.00658 -0.04717 0.00002" rpy="0 0 0"/>-->
            <mass value="0.44688"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
            <!--<inertia ixx="0.00030285" ixy="0.00000761" ixz="0.00000031" iyy="0.00022339" iyz="-0.00000001" izz="0.00039766" />-->
        </inertial>
    </link>
    
    <!-- Joint 5 -->
    <joint name="joint5" type="revolute">
        <parent link="link5"/>
        <child link="link6"/>
        <origin xyz="0.06300 0.04500 0.00000" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
        <dynamics damping="0.7"/>
    </joint>
    
    <transmission name="tran5">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint5">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor5">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    
    <!-- link 6 -->
    <link name="link6">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/link_6.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
        
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/link_6.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="grey"/>
        </visual>
        
        <inertial>
            <origin xyz="0.06831 -0.04457 0.00000" rpy="0 0 0"/>
            <!--<origin xyz="-0.05469 0.00043 0.00000" rpy="0 0 0"/>-->
            <mass value="0.43273"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
            <!--<inertia ixx="0.00021512" ixy="0.00001009" ixz="0.00000000" iyy="0.00042827" iyz="0.00000000" izz="0.00051436" />-->
        </inertial>
    </link>
    
    <!-- Joint 6 -->
    <joint name="joint6" type="revolute">
        <parent link="link6"/>
        <child link="end_link"/>
        <origin xyz="0.12300 -0.04500 0.00000" rpy="${-pi} 0 0"/>
        <axis xyz="1 0 0"/>
        <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
        <dynamics damping="0.7"/>
    </joint>
    
    <transmission name="tran6">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint6">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor6">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    
    <!-- end link -->
    <link name="end_link">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/collision/SAKE_Palm_Dual.stl" scale="1 1 1"/>
            </geometry>
        </collision>
        
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/visual/SAKE_Palm_Dual.stl" scale="1 1 1"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        
        <inertial>
            <origin xyz="0.0 0.00000 0.00000" rpy="0 0 0"/>
            <mass value="0.05"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
            <!--<inertia ixx="0.00000938" ixy="0.0" ixz="0.0" iyy="0.00000338" iyz="0.0" izz="0.00000635" />-->
        </inertial> 
    </link>
    
    <!-- Joint l1 -->
    <joint name="joint_l1" type="revolute">
        <parent link="end_link"/>
        <child link="prox_l"/>
        <origin xyz="0.1055 -0.0300 0.00000" rpy="${pi*0.5} 0 0"/>
        <!--         <origin xyz="0.1200 -0.0400 0.00000" rpy="${-pi*0.5} 0 0"/> -->
        <axis xyz="0 1 0"/>                                                  <!--UPDATE AXIS HERE LATER-->
        <limit velocity="6.5" effort="1000" lower="${-pi*0.45}" upper="${pi*0.1}" />
        <dynamics damping="0.1"/>
    </joint>
    
    <!-- proximal left phalanx  -->
    <link name="prox_l">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/prox.stl" scale="1 1 1"/>
            </geometry>
        </collision>
        
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/prox.stl" scale="1 1 1"/>
            </geometry>
            <material name="blue"/>
        </visual>
        
        <inertial>
            <origin xyz="0.0 0.00000 0.00000" rpy="0 0 0"/>
            <mass value="0.05"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial> 
    </link>
    
    <!-- Joint l2 -->
    <joint name="joint_l2" type="revolute">
        <parent link="prox_l"/>
        <child link="dist_l"/>
        <origin xyz="0.052 0.0000 0.00000" rpy="0 0 0"/>
        <!--         <origin xyz="0.1200 -0.0400 0.00000" rpy="${-pi*0.5} 0 0"/> -->
        <axis xyz="0 1 0"/>                                                 
        <limit velocity="6.5" effort="1000" lower="0" upper="${pi*0.65}" />
        <dynamics damping="0.1"/>
    </joint>
    
    <!-- distal left phalanx  -->
    <link name="dist_l">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://my_robotic_arm/meshes/dist.stl" scale="1 1 1"/>
            </geometry>
        </collision>
        
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
